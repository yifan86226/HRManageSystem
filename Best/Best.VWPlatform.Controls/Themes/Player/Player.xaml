                                <TextBlock x:Name="tbTotalTime" VerticalAlignment="Center" Height="18" FontFamily="MingLiu" FontSize="15" Text="{Binding TotalTime,Converter={StaticResource TimeValueConverter}}" TextWrapping="Wrap" Foreground="#FFFFFFFF" FontStyle="Normal" HorizontalAlignment="Left" TextAlignment="Right" Grid.Column="2"/>
                                <StackPanel Orientation="Horizontal" Grid.Column="3" HorizontalAlignment="Left" Visibility="Collapsed">
                                    <TextBlock  VerticalAlignment="Center"  Height="15" FontFamily="Microsoft YaHei" FontSize="12" Text="    " TextWrapping="Wrap" Foreground="#FFFFFFFF" HorizontalAlignment="Right" TextAlignment="Right" />
                                    <TextBlock  VerticalAlignment="Center"  Height="18" FontFamily="Microsoft YaHei" FontSize="15" Text="{Binding Speed,Converter={StaticResource SpeedValueConverter}}" TextWrapping="Wrap" Foreground="{Binding Speed,Converter={StaticResource SpeedColorConverter}}" FontStyle="Normal" HorizontalAlignment="Right" TextAlignment="Right" />
                                </StackPanel>
                            </Grid>

                            <Grid Grid.Column="1" Margin="0,0,0,0" HorizontalAlignment="Stretch" x:Name="gridCol2" VerticalAlignment="Bottom">
                                <mp:CustomSlider Cursor="Hand" ValueChangedCommand="{Binding ValueChangedCommand}" CurrTime="{Binding CurrTime,Mode=TwoWay}" TotalTime="{Binding TotalTime}"  AutoToolTipFormat="{}{0}"
                                                 AutoToolTipPlacement="TopLeft" AutoToolTipPrecision="3" HorizontalAlignment="Stretch"
                                                 Maximum="{Binding TotalTime}" LargeChange="0" SmallChange="0" x:Name="sliderTimeline"  Visibility="Visible"/>
                            </Grid>
                            <Grid Grid.Column="2" Margin="12,0,12,0" HorizontalAlignment="Stretch" x:Name="gridCol3" VerticalAlignment="Bottom">
                                <mp:Spinner Margin="0,0,0,0" x:Name="spinner" IsBusy="{Binding IsBusy}" Width="17" Height="17" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Grid>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>