<Window x:Class="CO_IA.UI.FreqPlan.FreqPlan.DialogFreqPartPlan"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:CO_IA.UI.FreqPlan.FreqPlan"
        Title="频率规划选择">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <local:FreqPartPlanList_Control x:Name="xfreqPartPlanList"/>
        <StackPanel Orientation="Horizontal" Margin="5" HorizontalAlignment="Right" Grid.Row="1">
            <Button Content="确定" Margin="3" MinWidth="60" x:Name="xbtnOk" Click="xbtnOk_Click"/>
            <Button Content="取消" Margin="3" MinWidth="60" x:Name="xbtnCalcel" Click="xbtnCalcel_Click"/>
        </StackPanel>
    </Grid>
</Window>
