<Window x:Class="CO_IA.UI.FreqPlan.FreqPlan.QueryFreqPartPlanDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
        xmlns:local="clr-namespace:CO_IA.UI.FreqPlan.FreqPlan"
        mc:Ignorable="d"  WindowStartupLocation ="CenterScreen"
        d:DesignHeight="300" d:DesignWidth="300"
        Title="周围台站获取">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <local:ActivityFreqPlanList_Control ShowBtnRoundStation="false" x:Name="xQueryfreqPartPlanList"/>
        <StackPanel Orientation="Horizontal" Grid.Row="1" HorizontalAlignment="Right" Margin="5">
            <Button Content=" 确定" x:Name="xbtnOk" Click="xbtnOk_Click"/>
            <Button Content=" 取消" x:Name="xbtnCancel" Click="xbtnCancel_Click"/>
        </StackPanel>
    </Grid>
</Window>
