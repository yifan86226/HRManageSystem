<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:CO_IA.UI.MAP" x:Class="CO_IA.UI.MAP.WinMapCoordinate"
        Title="地图选点" Height="600" Width="800" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" >
    <Grid x:Name="g">
        <Grid.RowDefinitions>
            <RowDefinition Height="173*"/>
            <RowDefinition Height="35"/>
        </Grid.RowDefinitions>

        <!--<local:MapCoordinate x:Name="mapCoord"/>-->
        <Grid  Grid.Row="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="9*"/>
                <ColumnDefinition Width="30"/>
                <ColumnDefinition Width="9*"/>
            </Grid.ColumnDefinitions>
            <Button x:Name="btnOK" Content="确认" Width="80" Height="24" HorizontalAlignment="Right" Click="btnOK_Click" ></Button>
            <Button x:Name="btnCancle" Content="取消" Width="80" Height="24" HorizontalAlignment="Left" Click="btnCancle_Click" Grid.Column="2"></Button>
        </Grid>
    </Grid>
</Window>
